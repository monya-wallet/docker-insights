{
  "network": "livenet",
  "port": 3001,
  "services": [
    "bitcoind",
    "insight-api",
    "insight-ui",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/root/.fujicoin/",
        "exec": "/home/node/fujicoind",
        "rpcuser": "user",
        "rpcpassword": "password"
      }
    },
    "insight-ui": {
      "routePrefix": "",
      "apiPrefix": "api"
    },
    "insight-api": {
      "routePrefix": "api",
      "enableCache": false
    }
  }
}
